{
  "name": "booking-analytics",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "bin": "server.js",
  "pkg": {
    "assets": [
      ".next/**/*.js"
    ],
    "scripts": [
      ".next/dist/**/*.js"
    ]
  },
  "scripts": {
    "test": "NODE_ENV=test jest",
    "dev2": "node server.js",
    "prod": "NODE_ENV=production node server.js",
    "build": "next build",
    "dev": "next -p 8080",
    "start": "npm run dev",
    "export": "next export",
    "next-run": "npm run build; npm run start",
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:ts": "tslint './app/**/*.ts*' --format stylish --force",
    "lint:sass": "stylelint ./app/**/*.scss --fix"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.4",
    "immer": "^1.12.1",
    "lodash": "^4.17.11",
    "next": "^7.0.2",
    "node-sass": "^4.10.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.126",
    "@types/materialize-css": "^1.0.5",
    "@types/next": "^7.0.4",
    "@types/react": "^16.7.4",
    "@types/react-redux": "^7.0.0",
    "@types/redux": "^3.6.0",
    "@types/redux-saga": "^0.10.5",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "jest": "^23.6.0",
    "jest-enzyme": "^7.0.1",
    "next-images": "^1.1.2",
    "prettier": "^1.16.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.6.3",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.6"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|js?|tsx?|ts?)$",
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/.build/",
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js"
  }
}
